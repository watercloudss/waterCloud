<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.watercloud.flash.mapper.FlashActivityMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.watercloud.flash.entity.FlashActivity">
        <id column="id" property="id" />
        <result column="activity_name" property="activityName" />
        <result column="activity_desc" property="activityDesc" />
        <result column="start_time" property="startTime" />
        <result column="end_time" property="endTime" />
        <result column="status" property="status" />
        <result column="modified_time" property="modifiedTime" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, activity_name, activity_desc, start_time, end_time, status, modified_time, create_time
    </sql>

</mapper>
